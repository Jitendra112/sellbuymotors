
<%- include ../adminlayouts/header.ejs %>
  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <h1>
        All Cars
        <small>Control panel</small>
      </h1>
      <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
        <li class="active">All Cars</li>
      </ol>
    </section>
   

    <table id="example" class="display" style="width:99%">
        <thead>
            <tr><th>S.No.</th>
                <th>Brand Name</th>
                <th>Model Name</th>
                <th style="width:30%">Car Image</th>
                
                
            </tr>
        </thead>
        <tbody>
          <% if (locals.data) { %>
              <% locals.data.forEach(function(cls,index){  %>
            <tr>
                 
                <td><%= index + 1 %></td>
                <td><%= cls.make %></td>
                <td><%= cls.model %></td>
                <td><img src="/uploads/<%= cls.image_name %>" style="width:30%"></td>
               <!--  <td><a href="/admin/Details">View Details</a></td> -->
                
            </tr>
               <% }) %>
                <% } %>
        </tbody>
     
    </table>
    
  </div>
  <!-- /.content-wrapper -->
 

 <%- include ../adminlayouts/footer.ejs %>
  <script>
    $(document).ready(function() {
    $('#example').DataTable();
} );
  </script>